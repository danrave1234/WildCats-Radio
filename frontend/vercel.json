{
  "rewrites": [
    {
      "source": "/oauth2/:path*",
      "destination": "https://api.wildcat-radio.live/oauth2/:path*"
    },
    {
      "source": "/login/oauth2/:path*",
      "destination": "https://api.wildcat-radio.live/login/oauth2/:path*"
    },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
} 